        {{ form_label(form, null, { 'label_attr': { 'class': 'form-label fw-bold' } }) }}
        {{ form_widget(form, { 'attr': { 'class': 'form-control' } }) }}
        {{ form_errors(form) }}
    </div>
{% endblock %}
