{% extends 'base.html.twig' %}

{% block title %}Project{% endblock %}

{% block body %}
    <h1>Project</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ project.id }}</td>
        </tr>
        <tr>
            <th>Title</th>
            <td>{{ project.title }}</td>
        </tr>
        <tr>
            <th>AmountOfGroups</th>
            <td>{{ project.amountOfGroups }}</td>
        </tr>
        <tr>
            <th>MaxStudentsPerGroup</th>
            <td>{{ project.maxStudentsPerGroup }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('app_project_index') }}">back to list</a>

    <a href="{{ path('app_project_edit', {'id': project.id}) }}">edit</a>
{% endblock %}
